    {{else}}
      {{#group.item route='session.agent.insights.help-center.articles' as |item|}}
        {{t 'insights.sidebar.help_center.articles.title'}}
      {{/group.item}}
    {{/if}}
  {{/sidebar.group}}

  {{#sidebar.group title=(t 'insights.sidebar.reporting.title') as |group|}}
    {{#if (variation "release-restrict-insights")}}
      {{#if hasCustomReportingInsights}}
        {{#group.item route='session.agent.insights.reporting.custom-reports' as |item|}}
          {{t 'insights.sidebar.reporting.custom_reports.title'}}
        {{/group.item}}
      {{else}}
        {{#ko-tooltip title=(t 'insights.sidebar.reporting.custom_reports.upgrade') delay=100}}
