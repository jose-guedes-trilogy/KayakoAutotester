            {{t 'insights.sidebar.sla.overview.title'}}
          {{/group.item}}
        {{/ko-tooltip}}
      {{/if}}
    {{/sidebar.group}}
  {{else}}
    {{#if hasSLAs}}
      {{#sidebar.group title=(t 'insights.sidebar.sla.title') as |group|}}
        {{#group.item route='session.agent.insights.sla.overview' as |item|}}
          {{t 'insights.sidebar.sla.overview.title'}}
        {{/group.item}}
      {{/sidebar.group}}
    {{/if}}
  {{/if}}

  {{#sidebar.group title=(t 'insights.sidebar.help_center.title') as |group|}}
