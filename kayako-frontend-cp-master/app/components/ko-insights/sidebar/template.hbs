{{#ko-sidebar as |sidebar|}}
  {{#sidebar.group title=(t 'insights.sidebar.general.title') as |group|}}
    {{#group.item route='session.agent.insights.general.cases' as |item|}}
      {{t 'insights.sidebar.general.cases.title'}}
    {{/group.item}}

    {{#if (variation "release-restrict-insights")}}
      {{#if hasAgentTeamInsights}}
        {{#group.item route='session.agent.insights.general.agents' as |item|}}
          {{t 'insights.sidebar.general.agents.title'}}
        {{/group.item}}
      {{else}}
        {{#ko-tooltip title=(t 'insights.sidebar.general.agents.upgrade') delay=100}}
          {{#group.item inactive=true as |item|}}
            {{t 'insights.sidebar.general.agents.title'}}
          {{/group.item}}
        {{/ko-tooltip}}
      {{/if}}
    {{else}}
      {{#group.item route='session.agent.insights.general.agents' as |item|}}
        {{t 'insights.sidebar.general.agents.title'}}
      {{/group.item}}
    {{/if}}

    {{#if (variation "release-restrict-insights")}}
      {{#if hasAgentTeamInsights}}
        {{#group.item route='session.agent.insights.general.teams' as |item|}}
          {{t 'insights.sidebar.general.teams.title'}}
        {{/group.item}}
      {{else}}
        {{#ko-tooltip title=(t 'insights.sidebar.general.teams.upgrade') delay=100}}
          {{#group.item inactive=true as |item|}}
            {{t 'insights.sidebar.general.teams.title'}}
          {{/group.item}}
        {{/ko-tooltip}}
      {{/if}}
    {{else}}
      {{#group.item route='session.agent.insights.general.teams' as |item|}}
        {{t 'insights.sidebar.general.teams.title'}}
      {{/group.item}}
    {{/if}}
  {{/sidebar.group}}

  {{#if (variation "release-restrict-insights")}}
    {{#sidebar.group title=(t 'insights.sidebar.sla.title') as |group|}}
      {{#if hasSLAs}}
        {{#group.item route='session.agent.insights.sla.overview' as |item|}}
