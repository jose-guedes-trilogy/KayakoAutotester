    "organizationRemoveIcon": "div[class*=\"ko-agent-content_sidebar_requester-info-card__cancel-icon_\"] .tooltipstered svg"
  },
  "identities": {
    "container": "div[class*=\"ko-identities__container_\"]",
    "list": "ul[class*=\"ko-identities__list_\"]",
    "identityTrigger": "div.ember-power-select-trigger[class*=\"ko-identities_identity__trigger_\"]",
    "identityEmail": "div[class*=\"ko-identities_identity_trigger__name_\"]",
    "identityCautionIcon": "div.tooltipstered svg[data-name=\"caution\"]",
    "identityChevron": "div[class*=\"ko-identities_identity_trigger__chevron_\"] svg",
