    "triggerInner": "div[class*=\"ko-user-cases__active-cases-trigger_\"]",
    "countIcon": "div[class*=\"ko-user-cases__conversations-count_\"] svg",
    "triggerText": "div[class*=\"ko-user-cases__trigger-text_\"]",
    "triggerChevron": "div[class*=\"ko-user-cases__chevron_\"] svg",
    "dropdown": "div[class*=\"ko-user-cases__sandbox-dropdown_\"]",
    "root": "div[class*=\"ko-user-cases_dropdown__root_\"]",
    "caseItem": "a[class*=\"ko-user-cases_dropdown__case-item_\"]",
    "caseItemInner": "div[class*=\"ko-user-cases_dropdown__case-item-inner_\"]",
    "titleRow": "div[class*=\"ko-user-cases_dropdown__title_\"]",
