    "viewAllLink": "div[class*=\"ko-user-cases_dropdown__view-all-cases_\"] a"
  },
  // Right-side field components (Assignee, Status, etc.)
  "infoBarCommon": {
    "powerSelectContainer": "div[class*=\"ko-info-bar_field_select__power-select_\"]",
    "selectTrigger": "div.ember-power-select-trigger[class*=\"ko-info-bar_field_select__trigger_\"], div.ember-power-select-trigger[class*=\"ko-info-bar_field_drill-down__trigger_\"]",
    "triggerCard": "div[class*=\"ko-info-bar_field_select_trigger__trigger_\"][class*=\"ko-info-bar_item__container_\"]",
    "triggerHeader": "span[class*=\"ko-info-bar_field_select_trigger__header_\"]",
    "triggerInput": "input[class*=\"ko-info-bar_field_select_trigger__input_\"]",
    "triggerPlaceholder": "span[class*=\"ko-info-bar_field_select_trigger__placeholder_\"]",
    "middleEllipsisSpan": "span[class*=\"ko-info-bar_field_select_trigger__middle-ellipsis-span_\"]",
    "dropdownContent": "div[id^=\"ember-basic-dropdown-content-\"][class*=\"ember-basic-dropdown-content\"]",
    "optionsList": "ul[id^=\"ember-power-select-options-\"]",
    "option": "li.ember-power-select-option"
  },
  "fields": {
    "assignee": {
      "container": "div[id^=\"ember\"][class*=\"ko-info-bar_field_drill-down__trigger_\"]",
      "triggerCard": "div[id^=\"ko-info-bar-select-trigger-ember\"][class*=\"ko-info-bar_field_select_trigger__trigger_\"]",
      "valueUnassigned": "div[class*=\"ko-case-content_field_assignee_trigger-value__trigger_\"]"
    },
    "status": {
      "container": "div[id^=\"ember\"][class*=\"ko-info-bar_field_select__power-select_\"] >> div.ember-power-select-trigger[class*=\"ko-info-bar_field_select__trigger_\"]",
      "swatchContainer": "div[class*=\"ko-case-content_field_status_swatch-with-label__container_\"]",
      "swatchOuter": "div[class*=\"ko-case-content_field_status_swatch-with-label__swatch_\"] div[class*=\"ko-status-swatch__outer_\"]",
      "label": "div[class*=\"ko-case-content_field_status_swatch-with-label__label_\"]"
    },
    "type": {
      "container": "div[id^=\"ember\"][class*=\"ko-info-bar_field_select__power-select_\"] >> div.ember-power-select-trigger[class*=\"ko-info-bar_field_select__trigger_\"]",
      "placeholder": "span[class*=\"ko-info-bar_field_select_trigger__placeholder_\"]",
      "middleEllipsis": "span[class*=\"ko-info-bar_field_select_trigger__middle-ellipsis-span_\"]"
    },
    "priority": {
      "container": "div[id^=\"ember\"][class*=\"ko-info-bar_field_select__power-select_\"] >> div.ember-power-select-trigger[class*=\"ko-info-bar_field_select__trigger_\"]",
      "placeholder": "span[class*=\"ko-info-bar_field_select_trigger__placeholder_\"]",
      "middleEllipsis": "span[class*=\"ko-info-bar_field_select_trigger__middle-ellipsis-span_\"]"
    },
    "tags": {
      "multiTrigger": "div.ember-power-select-multiple-trigger[class*=\"ko-info-bar_field_select-multiple__trigger_\"]",
      "triggerCard": "div[class*=\"ko-info-bar_field_select-multiple_trigger__trigger_\"][class*=\"ko-info-bar_item__container_\"]",
      "input": "input[class*=\"ko-info-bar_field_select-multiple_trigger__input_\"]",
      "statusIcon": "span.ember-power-select-status-icon"
    },
    "form": {
      "container": "div[id^=\"ember\"][class*=\"ko-info-bar_field_select__power-select_\"]",
      "arrowInset": "div[class*=\"ko-info-bar_field_select_arrow__arrow-inset_\"]",
      "arrow": "div[class*=\"ko-info-bar_field_select_arrow__arrow_\"]",
      "triggerCard": "div[id^=\"ko-info-bar-select-trigger-ember\"][class*=\"ko-info-bar_field_select_trigger__trigger_\"]",
      "middleEllipsis": "span[class*=\"ko-info-bar_field_select_trigger__middle-ellipsis-span_\"]"
    },
    "markAsSolvedByCustomer": {
      "container": "div[id^=\"ember\"][class*=\"ko-info-bar_field_select__power-select_\"] >> div.ember-power-select-trigger[class*=\"ko-info-bar_field_select__trigger_\"]",
      "placeholder": "span[class*=\"ko-info-bar_field_select_trigger__placeholder_\"]"
    },
    "equipmentType": {
      "container": "div[id^=\"ember\"][class*=\"ko-info-bar_field_drill-down__trigger_\"]",
      "triggerCard": "div[id^=\"ko-info-bar-select-trigger-ember\"][class*=\"ko-info-bar_field_select_trigger__trigger_\"]",
      "placeholder": "span[class*=\"ko-info-bar_field_select_trigger__placeholder_\"]"
    },
    "dateOfPurchase": {
      "container": "div[class*=\"ko-info-bar_field_date__date_\"][class*=\"ko-info-bar_item__container_\"]",
      "icon": "i[class*=\"ko-info-bar_field_date__icon_\"]",
      "focus": "span[class*=\"ko-info-bar_field_date__focus_\"]",
      "header": "span[class*=\"ko-info-bar_field_date__header_\"]",
      "value": "div[class*=\"ko-info-bar_field_date__date_\"] > span:not([class])"
    },
    "radioTest": {
      "container": "div[id^=\"ember\"][class*=\"ko-info-bar_field_select__power-select_\"] >> div.ember-power-select-trigger[class*=\"ko-info-bar_field_select__trigger_\"]",
      "placeholder": "span[class*=\"ko-info-bar_field_select_trigger__placeholder_\"]"
    },
    "testDropdown": {
      "container": "div[id^=\"ember\"][class*=\"ko-info-bar_field_select__power-select_\"] >> div.ember-power-select-trigger[class*=\"ko-info-bar_field_select__trigger_\"]",
      "placeholder": "span[class*=\"ko-info-bar_field_select_trigger__placeholder_\"]"
    },
    "warrantyClaimsReason": {
      "container": "div[class*=\"ko-info-bar_field_text__text_\"][class*=\"ko-info-bar_item__container_\"]",
      "header": "span[class*=\"ko-info-bar_field_text__header_\"]",
      "input": "input[class*=\"ko-info-bar_field_text__input_\"]"
    }
  },
  // Shared dropdown scaffolding
  "dropdownCommons": {
    "content": "div[id^=\"ember-basic-dropdown-content-\"][class*=\"ember-basic-dropdown-content\"]",
    "placeholderContent": "div[id^=\"ember-basic-dropdown-content-\"][class*=\"ember-basic-dropdown-content-placeholder\"]",
    "optionsList": "ul[id^=\"ember-power-select-options-\"]",
    "option": "li.ember-power-select-option",
    "group": "li.ember-power-select-group",
    "groupName": "span.ember-power-select-group-name"
  },
  "misc": {
    "genericIcon": "svg[class*=\"ko-icon__icon_\"]",
    "tooltipHost": ".tooltipstered"
  }
}

