// Agent conversation right-side Properties panel (info bar/fields)
// Includes requester card, identities, per-field selectors, and shared dropdown structures
{
  "layout": {
    "sidebar": "div[class*=\"ko-agent-content_layout__sidebar_\"]",
    "fieldsPane": "div[class*=\"ko-agent-content_layout__fields_\"]",
    "spacer": "div[class*=\"ko-agent-content_layout__spacer_\"]",
    "submitButtonRegion": "div[class*=\"ko-agent-content_layout__submit-button_\"]"
  },
  "requesterCard": {
    "blockContainer": "div[class*=\"ko-case-content_info-block__container_\"]",
    "infoCard": "div[class*=\"ko-agent-content_sidebar_requester-info-card__info-card_\"]",
    "avatarLink": "a[href*=\"/agent/conversations/\"][href$=\"/user\"]",
    "avatarContainer": "div[class*=\"ko-user-avatar__avatar-container_\"]",
    "avatarImageMedium": "img[class*=\"ko-user-avatar_image__image_\"][class*=\"ko-user-avatar_image__sizeMedium_\"]",
    "overview": "div[class*=\"ko-agent-content_sidebar_requester-info-card__requester-overview_\"]",
    "nameRow": "div[class*=\"ko-agent-content_sidebar_requester-info-card__requester-name_\"]",
    "nameEditableContainer": "div[class*=\"ko-editable-text__container_\"]",
    "nameText": "div[class*=\"ko-editable-text__text_\"]",
    "nameEmphasized": "span[class*=\"ko-editable-text__emphasizedText_\"]",
    "namePencilIcon": "span[class*=\"ko-editable-text__pencil_\"] svg",
