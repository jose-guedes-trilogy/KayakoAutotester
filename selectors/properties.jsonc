    "requesterName": "div[class*=\"ko-user-cases_dropdown__name_\"]:not([class*=\"__unassigned_\"])",
    "assigneeUnassigned": "div[class*=\"ko-user-cases_dropdown__name_\"][class*=\"__unassigned_\"]",
    "statusRow": "div[class*=\"ko-user-cases_dropdown__status_\"]",
    "statusPillNew": "div[class*=\"ko-user-cases_dropdown__case-status_\"][class*=\"__case-status-NEW_\"]",
