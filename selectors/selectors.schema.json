{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "patternProperties": {
    "^[A-Za-z0-9_.-]+$": {
      "type": "object",
      "patternProperties": {
        "^[A-Za-z0-9_.-]+$": {
          "oneOf": [
            {
              "type": "array",
              "items": { "type": "string", "minLength": 1 },
              "minItems": 1
            },
            { "type": "string", "minLength": 1 }
          ]
        }
      },
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}





